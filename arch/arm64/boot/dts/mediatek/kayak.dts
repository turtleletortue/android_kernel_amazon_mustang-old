/* Main Cam */
&kd_camera_hw1 {

	reg-vcama-supply = <&mt6323_vcama_reg>;
	reg-vcamd-supply = <&mt6323_vcamd_reg>;
	reg-vcamio-supply = <&mt6323_vcamio_reg>;
	reg-vcamaf-supply = <&mt6323_vcamaf_reg>;
	reg-vcami2c-supply = <&mt6323_vio18_reg>;

	pinctrl-names = "default",
		"cam0_rst0", "cam0_rst1", "cam0_pnd0", "cam0_pnd1",
		"cam1_rst0", "cam1_rst1", "cam1_pnd0", "cam1_pnd1",
		"cam_mclk",
		"cam0_rdn0",  "cam0_rdp0", "cam0_rcn", "cam0_rcp",
		"cam1_rcn_a",  "cam1_rcp_a", "cam1_rdn0_a", "cam1_rdp0_a";

	pinctrl-0 = <&camera_pins_default>;
	pinctrl-1 = <&camera_pins_cam0_rst0>;
	pinctrl-2 = <&camera_pins_cam0_rst1>;
	pinctrl-3 = <&camera_pins_cam0_pnd0>;
	pinctrl-4 = <&camera_pins_cam0_pnd1>;
	pinctrl-5 = <&camera_pins_cam1_rst0>;
	pinctrl-6 = <&camera_pins_cam1_rst1>;
	pinctrl-7 = <&camera_pins_cam1_pnd0>;
	pinctrl-8 = <&camera_pins_cam1_pnd1>;

	pinctrl-9 = <&camera_pins_cam_mclk>;

	pinctrl-10 = <&camera_pins_cam0_rdn0>;
	pinctrl-11 = <&camera_pins_cam0_rdp0>;
	pinctrl-12 = <&camera_pins_cam0_rcn>;
	pinctrl-13 = <&camera_pins_cam0_rcp>;

	pinctrl-14 = <&camera_pins_cam1_rcn_a>;
	pinctrl-15 = <&camera_pins_cam1_rcp_a>;
	pinctrl-16 = <&camera_pins_cam1_rdn0_a>;
	pinctrl-17 = <&camera_pins_cam1_rdp0_a>;

	status = "okay";

};

