		reg = <0x72>;
		tmp103-sensor,offset = <750>;
		tmp103-sensor,alpha = <10>;
		tmp103-sensor,weight = <0>;
	};

	/* Accel Sensor */
	g_sensor@4c {
		compatible = "mediatek,mc3416";
		reg = <0x4c>;
		status = "okay";
                i2c_num = <2>;
                i2c_addr = <0x4c 0 0 0>;
                direction = <6>;
                power_id = <0xffff>;
                power_vol = <0>;
                firlen = <0>;
                is_batch_supported = <0>;
	};

	g_sensor@18 {
		compatible = "mediatek,bma_gsensor";
		reg = <0x18>;
		status = "okay";
                i2c_num = <2>;
                i2c_addr = <0x18 0 0 0>;
                direction = <6>;
                power_id = <0xffff>;
                power_vol = <0>;
                firlen = <0>;
                is_batch_supported = <0>;
	};

	/* Gyro Sensor */
	gyro_sensor@6c {
		compatible = "mediatek,gyro_sensor";
		reg = <0x6c>;
	};

