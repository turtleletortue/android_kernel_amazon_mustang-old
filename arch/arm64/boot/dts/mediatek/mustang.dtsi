#include "mt8163.dtsi"
#include "bat_setting/mustang_bat_setting.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	memory@00000000 {
		device_type = "memory";
		reg = <0 0x40000000 0 0x20000000>;
	};

	keypanic {
		compatible = "amzn,keypanic";
		keys_down = "KEY_VOLUMEDOWN", "KEY_VOLUMEUP", "KEY_POWER";
		keys_down_delay = <5000>;
		status = "okay";
	};
	/* Accel Sensor */
	cust_gsensor@0 {
		compatible = "mediatek,mc3416";
		i2c_num = <2>;
		i2c_addr = <0x4c>;
		direction = <6>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <0>;
		is_batch_supported = <0>;
	};

	cust_gsensor@1 {
		compatible = "mediatek,bma253";
		i2c_num = <2>;
		i2c_addr = <0x18>;
		direction = <6>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <0>;
		is_batch_supported = <0>;
	};


	/* Gyro Sensor */
	cust_gyroscope@0 {
		compatible = "mediatek,lsm6ds3gy";
		i2c_num = <2>;
		i2c_addr = <0x6a>;
		direction = <0>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <0>;
		is_batch_supported = <0>;
	};

	/* chosen */
	chosen {
		bootargs = "console=ttyS0,921600n1 root=/dev/ram initrd=0x44000200,0x200000";
	};

        firmware: firmware {
                android: android {
                        compatible = "android,firmware";
                        fstab: fstab {
                                compatible = "android,fstab";
                                vendor {
                                        compatible = "android,vendor";
                                        dev =
                "/dev/block/platform/soc/11230000.mmc/by-name/vendor";
                                        type = "ext4";
                                        mnt_flags = "ro";
                                        #ifndef CONFIG_MTK_DM_VERITY_OFF
                                        fsmgr_flags = "wait,verify";
                                        #else
                                        fsmgr_flags = "wait";
                                        #endif
                                };
                        };
                };
        };

	mtcpufreq {
		compatible = "mediatek,mt8163-cpufreq";
		reg-ext-vproc-supply = <&vproc>;
	};
        keypanic {
		compatible = "amzn,keypanic";
		keys_down = "KEY_VOLUMEDOWN", "KEY_VOLUMEUP", "KEY_POWER";
		keys_down_delay = <5000>;
		status = "okay";
	};
	/* Hall Sensor */
	hall_sensors {
		compatible = "rohm,hall-ah9250";
		interrupt-parent = <&pio>;
		interrupts = <86 IRQ_TYPE_LEVEL_LOW>;
		int-gpio = <&pio 85 0>;
		sensor-name = "hall_sensor_1";
		sensor-num = <1>;
	};
	/* ALS */
	cust_alsps@0 {
		compatible		= "mediatek,ltr559";
		i2c_num			= <2>;
		i2c_addr		= <0x23>;
		polling_mode_ps		= <0>;
		polling_mode_als	= <1>;
		power_id		= <0xffff>;
		power_vol		= <0>;
		/* Total has 15 level*/
		als_level	= <0 257 637 1027 2297 5588 6730 11301 20449 25768 31122 45552 57937 57937 57937>;
		/*  Total has 16 range*/
		als_value	= <0 132 304 502 1004 2003 3006 5006 8004 10000 12000 16000 20000 20000 20000 20000>;
		ps_threshold_high	= <70>;
		ps_threshold_low	= <40>;
		is_batch_supported_ps	= <0>;
		is_batch_supported_als   = <0>;
	};

	/* auxadc thermistor channel */
	thermal-sensor {
		compatible = "generic-adc-thermal";
		#thermal-sensor-cells = <0>;
		io-channels = <&auxadc 0>, <&auxadc 1>, <&auxadc 15>;
		io-channel-names = "sensor-channel";
	};

	/* Virtual sensor*/
	/* select_device: Decide to register which thermal_zone device * /
	/* 0: register to thermal_zone9, 1: register to thermal_zone10 */
	virtual_sensor@0{
		compatible = "amazon,virtual_sensor-thermal";
		dev_id = <0>;
		num_trips = <9>;
		mode = <0>;
		polling_delay = <3000>;
		governor_name = "virtual_sensor";
		num_tbps = <3>;
		temp = <44000 44500 45000 45500 49000 52000 53000 54000 56000 56000 56000 56000>;
		type = <0 0 0 0 0 0 0 0 3 3 3 3>;
		hyst = <0 0 0 0 0 0 0 3000 0 0 0 0>;
		num_cdevs = <2>;
		cdev_names = "thermal_budget0","lcd-backlight0";
	};
	virtual_sensor@1{
		compatible = "amazon,virtual_sensor-thermal";
		dev_id = <1>;
		num_trips = <9>;
		mode = <0>;
		polling_delay = <3000>;
		governor_name = "virtual_sensor";
		num_tbps = <3>;
		temp = <49000 49500 50000 50500 53000 56000 57000 58000 60000 60000 60000 60000>;
		type = <0 0 0 0 0 0 0 0 3 3 3 3>;
		hyst = <0 0 0 0 0 0 0 3000 0 0 0 0>;
		num_cdevs = <2>;
		cdev_names = "thermal_budget1","bcct1";
	};
    /*Virtual sensor wmt*/
	thermal_wmt@0{
		compatible = "amazon,mtkswmt_sensor";
		thermal_wmt,offset = <100>;
		thermal_wmt,alpha = <101>;
		thermal_wmt,weight = <0>;
		select_device = <0>;
	};
	thermal_wmt@1{
		compatible = "amazon,mtkswmt_sensor";
		thermal_wmt,offset = <100>;
		thermal_wmt,alpha = <101>;
		thermal_wmt,weight = <0>;
		select_device = <1>;
	};
	/*Virtual sensor battery*/
	thermal_battery@0{
		compatible = "amazon,mtksbattery_sensor";
		thermal_battery,offset = <41500>;
		thermal_battery,offset_invert = <0>;
		thermal_battery,alpha = <721>;
		thermal_battery,weight = <43>;
		select_device = <0>;
	};
	thermal_battery@1{
		compatible = "amazon,mtksbattery_sensor";
		thermal_battery,offset = <43900>;
		thermal_battery,offset_invert = <0>;
		thermal_battery,alpha = <720>;
		thermal_battery,weight = <16>;
		select_device = <1>;
	};
	/*Virtual sensor gpu*/
	thermal_gpu@0{
		compatible = "amazon,mtksgpu_sensor";
		thermal_gpu,offset = <100>;
		thermal_gpu,alpha = <104>;
		thermal_gpu,weight = <0>;
		select_device = <0>;
	};
	thermal_gpu@1{
		compatible = "amazon,mtksgpu_sensor";
		thermal_gpu,offset = <100>;
		thermal_gpu,alpha = <104>;
		thermal_gpu,weight = <0>;
		select_device = <1>;
	};
	/*Virtual sensor pmic*/
	thermal_pmic@0{
		compatible = "amazon,mtkspmic_sensor";
		thermal_pmic,offset = <66200>;
		thermal_pmic,alpha = <416>;
		thermal_pmic,weight = <18>;
		thermal_pmic,weight_invert = <1>;
		select_device = <0>;
	};
	thermal_pmic@1{
		compatible = "amazon,mtkspmic_sensor";
		thermal_pmic,offset = <63900>;
		thermal_pmic,alpha = <417>;
		thermal_pmic,weight = <0>;
		thermal_pmic,weight_invert = <0>;
		select_device = <1>;
	};
	/*Virtual sensor cpu*/
	thermal_cpu@0{
		compatible = "amazon,mtkscpu_sensor";
		thermal_cpu,offset = <100>;
		thermal_cpu,alpha = <103>;
		thermal_cpu,weight = <0>;
		select_device = <0>;
	};
	thermal_cpu@1{
		compatible = "amazon,mtkscpu_sensor";
		thermal_cpu,offset = <100>;
		thermal_cpu,alpha = <103>;
		thermal_cpu,weight = <0>;
		select_device = <1>;
	};
	/* Virtual sensor thermistor */
	thermistor@00 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <49400>;
		thermal_thermistor,offset_invert = <0>;
		thermal_thermistor,alpha = <1>;
		thermal_thermistor,weight = <61>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <0>;
		select_device = <0>;
	};
	thermistor@01 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <55300>;
		thermal_thermistor,offset_invert = <0>;
		thermal_thermistor,alpha = <1>;
		thermal_thermistor,weight = <0>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <0>;
		select_device = <1>;
	};
	thermistor@10 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <55700>;
		thermal_thermistor,offset_invert = <0>;
		thermal_thermistor,alpha = <302>;
		thermal_thermistor,weight = <0>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <1>;
		select_device = <0>;
	};
	thermistor@11 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <55700>;
		thermal_thermistor,offset_invert = <0>;
		thermal_thermistor,alpha = <302>;
		thermal_thermistor,weight = <0>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <1>;
		select_device = <1>;
	};
	thermistor@20 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <9350>;
		thermal_thermistor,offset_invert = <1>;
		thermal_thermistor,alpha = <178>;
		thermal_thermistor,weight = <735>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <2>;
		select_device = <0>;
	};
	thermistor@21 {
		compatible = "amazon,virtual_sensor_thermistor";
		thermal_thermistor,offset = <8120>;
		thermal_thermistor,offset_invert = <1>;
		thermal_thermistor,alpha = <236>;
		thermal_thermistor,weight = <824>;
		thermal_thermistor,weight_invert = <0>;
		aux_channel_num = <2>;
		select_device = <1>;
	};

	led@6 {
		compatible = "mediatek,lcd-backlight";
		led_mode = <5>;
		data = < >;
		pwm_config = <0 0 0 0 0>;
		gpios = <&pio 59 0>;
		brightness_normalize_param = < >;
	};

	/*mustang:bais_pwr_en gpio32,rst gpio88 */
	lcm0: lcm@0{
		compatible = "lcm,lcm_dts_otm7290b_mustang";
		pinctrl-names = "default";
		lcm_pwr_gpio = <&pio 32 0>;
		lcm_reset_gpio = <&pio 88 0>;
		reg-lcm-supply = <&mt6323_vgp1_reg>;
		status = "okay";
	};

	keysilent-ota {
		compatible = "amzn,silent-ota";
		keys_down = "KEY_POWER";
		keys_down_delay = <0>;
		status = "okay";
	};

	integrity-module {
		compatible = "amzn,integrity-module";
		status = "okay";
	};
};

&accdet {
	interrupt-parent = <&pio>;
	interrupts = <4 IRQ_TYPE_LEVEL_LOW>;
	eint-debounce = <256000>;
	accdet-gpio = <&pio 26 0>;
	hp-sw1-pin = <&pio 90 1>;
	hp-sw2-pin = <&pio 87 0>;
	mic_dangling_vol = <1650>;
	mic_voltage_delay = <80>;
	switch-mode-vol = <550>;
	switch-mode-delay = <500>;
	accdet-mic-vol = <7>;
	headset-mode-setting = <0x500 0x500 1 0x1f0 0x800 0x800 0x4000>;
	accdet-plugout-debounce = <1000>;
	/*1:ACC mode, 2:low cost without in bias, 6:low cost with in bias*/
	accdet-mic-mode = <1>;
	/*0:micbias1 is controlled by reg 0x710, for abg123 & abf123 & old projects*/
	/*1:micbias1 is controlled by both reg 0x710 & 0x77A, for abc123 & mustang*/
	accdet-micbias1-ind-ctl = <1>;
	/*0--MD_MAX--UP_MAX--DW_MAX*/
	headset-three-key-threshold = <0 100 220 500>;
	/*0--MD_MAX--VOICE_MAX--UP_MAX--DW_MAX*/
	headset-four-key-threshold = <0 58 121 192 450>;
	pinctrl-names = "default", "state_eint_as_int";
	pinctrl-0 = <&accdet_pins_default>;
	pinctrl-1 = <&accdet_pins_eint_as_int>;
	status = "okay";
};

&audiosys {
	/*0:Stereo, 1:MonoLeft, 2:MonoRight*/
	channel-type = <0>;
	switch-mode-delay = <1>;
	sound-dai = <&rt551x>;
};

&audgpio {
	pinctrl-names = "default",
			"audpmicclk-mode0",
			"audpmicclk-mode1";
	pinctrl-0 = <&aud_pins_default>;
	pinctrl-1 = <&aud_pins_pmicclk_mode0>;
	pinctrl-2 = <&aud_pins_pmicclk_mode1>;
	status = "okay";
};

&auxadc {
	status = "okay";
};

&consys {
	pinctrl-names = "default",
			"gps_lna_state_init",
			"gps_lna_state_oh",
			"gps_lna_state_ol",
			"wifi_reset_init";
	pinctrl-0 = <&consys_pins_default>;
	pinctrl-1 = <&gpslna_pins_init>;
	pinctrl-2 = <&gpslna_pins_oh>;
	pinctrl-3 = <&gpslna_pins_ol>;
	pinctrl-4 = <&wifi_reset_init>;
	vcn18-supply = <&mt6323_vcn18_reg>;
	vcn28-supply = <&mt6323_vcn28_reg>;
	vcn33_bt-supply = <&mt6323_vcn33_bt_reg>;
	vcn33_wifi-supply = <&mt6323_vcn33_wifi_reg>;
	status = "okay";
};

&alsps {
	pinctrl-names = "pin_default", "pin_cfg";
	pinctrl-0 = <&alsps_intpin_default>;
	pinctrl-1 = <&alsps_intpin_cfg>;
	status = "okay";
};

&i2c0 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c0_pins_a>;
	status = "okay";

	lp8557_led@2c {
		compatible = "ti,lp855x-led";
		reg = <0x2c>;
		epprom_data = /bits/ 8 <0x14 0x07
					0x13 0x01
					0x11 0x05
					0x12 0x2B
					0x15 0x42
					0x16 0xA0>;
	};

	/* in cust_i2c_projXXX.dtsi ? */
	kd_camera_hw1@3c { /* Fix me to right reg val */
		compatible = "mediatek,camera_main";
		reg = <0x3c>;
	};

	/* in cust_i2c_projXXX.dtsi ? */
	kd_camera_hw2@21 { /* Fix me to right reg val */
		compatible = "mediatek,camera_sub";
		reg = <0x21>;
	};

	rt551x: audio-codec@57 {
		compatible = "realtek,rt551x";
		#sound-dai-cells = <0>;
		reg = <0x57>;
		interrupt-parent = <&pio>;
		interrupts = <6 IRQ_TYPE_LEVEL_HIGH>;
		rt5518-irq = <&pio 28 0>;
		rt5518-reset = <&pio 51 0>;
		pinctrl-names = "state_eint_as_int";
		pinctrl-0 = <&rt551x_pins_eint_as_int>;
		hw_analog_gain = <0xe>;
		hw_digital_gain = <0x883b>;
	};
};

&i2c1 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c1_pins_a>;
	status = "okay";

	vproc: sym827@60 {
		compatible = "silergy,sym827-regulator";
		reg = <0x60>;
		regulator-name = "vproc";
		regulator-min-microvolt = <600000>;
		regulator-max-microvolt = <1387500>;
		regulator-enable-ramp-delay = <400>;
		regulator-ramp-delay = <8333>;
		regulator-always-on;
		vsel-gpio = <&pio 34 0>;
	};
};

&i2c2 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c2_pins_a>;
	status = "okay";

	bq24296@6b {
		compatible = "ti,bq24297";
		reg = <0x6b>;
		interrupt-parent = <&pio>;
		interrupts = <11 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "bq24297_chg_en", "bq24297_interrupt_init";
		pinctrl-0 = <&bq24297_chg_en_pin>;
		pinctrl-1 = <&bq24297_int_pin>;
	};
	/* light sensor LTR559 */
	alsps@23 {
		compatible = "mediatek,alsps";
		reg = <0x23>;
	};

	tmp103-sensor@70 {
		compatible = "ti,tmp103_temp_sensor";
		reg = <0x70>;
		tmp103-sensor,offset = <2300>;
		tmp103-sensor,alpha = <10>;
		tmp103-sensor,weight = <0>;
	};
	tmp103-sensor@71 {
